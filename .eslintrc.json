{
  "env": {
    "node": true,
    "es2021": true,
    "commonjs": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "double"],
    "no-undef": 0,
    "comma-dangle": 0,
    "operator-linebreak": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "no-unused-vars": 0,
    "implicit-arrow-linebreak": 0,
    "function-paren-newline": 0,
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "linebreak-style": 0,
    "object-curly-newline": 0,
    "no-restricted-syntax": 0,
    "indent": 0,
    "no-plusplus": 0
  }
}
